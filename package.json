{
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint:eslint": "eslint \"src/ts/*.ts\"",
    "lint:eslint:fix": "eslint \"src/ts/*.ts\" --fix",
    "lint:prettier": "prettier \"**/*.{css,js,scss,ts,yaml,yml}\" --check",
    "lint:prettier:fix": "prettier \"**/*.{css,js,scss,ts,yaml,yml}\" --write",
    "lint:stylelint": "stylelint \"src/scss/*.scss\"",
    "lint:stylelint:fix": "stylelint \"src/scss/*.scss\" --fix",
    "lint": "npm run lint:eslint && npm run lint:stylelint && npm run lint:prettier",
    "lint:fix": "npm run lint:eslint:fix && npm run lint:stylelint:fix && npm run lint:prettier:fix",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass": "^1.90.0",
    "sharp": "^0.34.3",
    "stylelint": "^16.23.1",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "stylelint-order": "^7.0.0",
    "stylelint-plugin-defensive-css": "^1.0.4",
    "svgo": "^4.0.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "vite": "^7.1.3",
    "vite-plugin-image-optimizer": "^2.0.2",
    "vite-plugin-static-copy": "^3.1.2"
  },
  "engines": {
    "node": ">=21.2.0"
  },
  "private": true,
  "type": "module"
}
